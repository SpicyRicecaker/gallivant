{
  "name": "gallivant",
  "version": "0.1.0",
  "description": "Search All The Engines, With One Keystroke",
  "type": "module",
  "scripts": {
    "start": "vite",
    "watch": "vite build --watch",
    "serve:firefox": "web-ext run --start-url about:debugging#/runtime/this-firefox --source-dir ./dist/ --firefox firefoxdeveloperedition",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://github.com\" --source-dir ./dist/",
    "tsc": "tsc",
    "make": "vite build",
    "test": "vitest",
    "lint": "eslint .",
    "webext:lint": "web-ext lint -s ./dist",
    "dist": "web-ext build --source-dir=./dist --overwrite-dest",
    "product": "bun make && bun dist"
  },
  "postcss": {
    "plugins": {
      "postcss-mixins": {},
      "postcss-nested": {}
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/fs-extra": "^9.0.13",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "c8": "^7.12.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-solid": "^0.7.1",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "postcss-mixins": "^9.0.3",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.7.1",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.22.0",
    "web-ext": "^7.2.0",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "solid-icons": "^1.0.1",
    "solid-js": "^1.4.8"
  }
}