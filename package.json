{
  "name": "gallivant",
  "version": "0.1.0",
  "description": "Search All The Engines, With One Keystroke",
  "type": "module",
  "scripts": {
    "start": "vite",
    "watch": "vite build --watch",
    "serve:firefox": "web-ext run --start-url about:debugging#/runtime/this-firefox --source-dir ./dist/ --firefox firefoxdeveloperedition",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://github.com\" --source-dir ./dist/",
    "tsc": "tsc",
    "make": "vite build",
    "test": "vitest",
    "lint": "eslint .",
    "webext:lint": "web-ext lint -s ./dist",
    "dist": "web-ext build --source-dir=./dist --overwrite-dest",
    "bun:product": "bun make && bun dist",
    "pnpm:product": "pnpm make && pnpm dist"
  },
  "license": "MIT",
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^2.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/fs-extra": "^9.0.13",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webextension-polyfill": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "c8": "^7.12.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-solid": "^0.8.0",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.2",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "solid-testing-library": "^0.3.0",
    "typescript": "*",
    "vite": "^3.2.2",
    "vite-plugin-solid": "^2.3.10",
    "vitest": "^0.24.5",
    "web-ext": "^7.3.1",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "solid-icons": "^1.0.4",
    "solid-js": "^1.6.1"
  }
}